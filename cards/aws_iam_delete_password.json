{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.2",
    "body": [
        {
            "type": "TextBlock",
            "text": "Which keys do you want to delete?",
            "wrap": true
        },
        {
            "type": "Input.ChoiceSet",
            "id": "keyId",
            "isMultiSelect": false,
            "style": "expanded",
            "choices": 
                {{ key_choices }}
            ,
            "placeholder": "Placeholder text"
        }
    ],
    "actions": [
        {
            "type": "Action.Submit",
            "id": "cancel",
            "title": "Cancel",
            "data": {"isSubmit":false, "card_dialogue_index": "delete_aws_iam_password"}
        },
        {
            "type": "Action.Submit",
            "id": "submit",
            "title": "Submit",
            "data": {"isSubmit":true, "card_dialogue_index": "delete_aws_iam_password"}
        }
    ]
}